<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anurag.iiitvApp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.anurag.iiitvApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.anurag.iiitvApp.SemesterActivity" />
        <activity android:name="com.example.anurag.iiitvApp.CourseNameActivity" />
        <activity android:name="com.example.anurag.iiitvApp.BranchActivity" />
        <activity android:name="com.example.anurag.iiitvApp.CourseDataActivity" />
        <activity android:name="com.example.anurag.iiitvApp.CourseDetailActivity" />
        <activity android:name="com.example.anurag.iiitvApp.QAActivity" />
        <activity android:name="com.example.anurag.iiitvApp.Answer" />
        <activity android:name="com.example.anurag.iiitvApp.addAnswerActivity" />
        <activity android:name="com.example.anurag.iiitvApp.addThoughtActivity" />
        <activity android:name="com.example.anurag.iiitvApp.IIITVMainActivity" />
        <activity android:name="com.example.anurag.iiitvApp.LoginActivity"></activity>
    </application>

</manifest>